<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
    </application>
    <!-- FASE LISTENER  -->
    <lifecycle>
        <phase-listener>
            br.com.tesouro.controll.util.PhaseListener
        </phase-listener>
    </lifecycle>
       
            
    <!-- BEANS -->
    <managed-bean>
        <managed-bean-name>agenciaController</managed-bean-name>
        <managed-bean-class>
            br.com.tesouro.controll.AgenciaController
        </managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>  
    
    <managed-bean>
        <managed-bean-name>cidadeController</managed-bean-name>
        <managed-bean-class>
            br.com.tesouro.controll.CidadeController
        </managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean> 
    
    <managed-bean>
        <managed-bean-name>bancoController</managed-bean-name>
        <managed-bean-class>
            br.com.tesouro.controll.BancoController
        </managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean> 
        
</faces-config>
